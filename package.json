  "scripts": {
    "contracts:compile:abi": "typechain --target ethers-v5 --out-dir src/abis/types \"./src/abis/**/*.json\"",
    "contracts:compile:uniswaphelpers": "typechain --target ethers-v5 --out-dir src/abis/types/uniswap \"./node_modules/@uniswap/v3-periphery/artifacts/contracts/lens/UniswapInterfaceMulticall.sol/UniswapInterfaceMulticall.json\"",
    "contracts:compile": "yarn contracts:compile:abi && yarn contracts:compile:uniswaphelpers",
    "prepare": "yarn contracts:compile",
    "start": "react-app-rewired start",
    "start:cypress": "env REACT_APP_IS_CYPRESS='true' react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "cypress": "start-server-and-test 'yarn start:cypress' http://localhost:3000 'cypress open --browser chrome --e2e'",
    "cypress:headless": "yarn build && start-server-and-test 'yarn start' http://localhost:3000 'cypress run --browser chrome --e2e'",
    "lint": "eslint --fix --ext .js,.jsx,.tsx ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
